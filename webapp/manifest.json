{
    "_version": "1.14.0",
    "sap.app": {
        "id": "zuimatmaster",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap-ux/fiori-freestyle-writer:basic",
            "version": "0.11.15",
            "toolsId": "e9576319-15e6-43fa-ab37-93e98c1f6e48"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZGW_3DERP_MATMASTER_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "annotations": [],
                    "localUri": "localService/mainService/metadata.xml"
                }
            },
            "ZGW_3DERP_MATERIAL_SRV": {
                "uri": "/sap/opu/odata/sap/ZGW_3DERP_MATERIAL_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "annotations": [],
                    "localUri": "localService/ZGW_3DERP_MATERIAL_SRV/metadata.xml"
                }
            },
            "ZGW_3DERP_COMMON_SRV": {
                "uri": "/sap/opu/odata/sap/ZGW_3DERP_COMMON_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "annotations": [],
                    "localUri": "localService/ZGW_3DERP_COMMON_SRV/metadata.xml"
                }
            },
            "ZGW_3DERP_RFC_SRV": {
                "uri": "/sap/opu/odata/sap/ZGW_3DERP_RFC_SRV",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "annotations": [],
                    "localUri": "localService/ZGW_3DERP_RFC_SRV/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.71.49",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "commands": {
            "Save": {
                "shortcut": "Ctrl+S"
            },
            "Delete": {
                "shortcut": "Ctrl+D"
            },
            "Edit": {
                "shortcut": "Ctrl+E"
            },
            "New": {
                "shortcut": "Ctrl+I"
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "zuimatmaster.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "ZGW_3DERP_MATERIAL_SRV": {
                "dataSource": "ZGW_3DERP_MATERIAL_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            },
            "ZGW_3DERP_COMMON_SRV": {
                "dataSource": "ZGW_3DERP_COMMON_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            },
            "ZGW_3DERP_RFC_SRV": {
                "dataSource": "ZGW_3DERP_RFC_SRV",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "zuimatmaster.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": ":sbu:/:matno:",
                    "target": [
                        "TargetMain"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Main",
                    "viewName": "Main"
                }
            }
        },
        "rootView": {
            "viewName": "zuimatmaster.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "config": {
            "fullWidth": true
        }
    }
}